{
    "special-font-letters": {
        "blackboard-bold": {
            "prefix": "b",
            "command": "mathbb",
            "lower": false,
            "alternative": ["f"]
        },

        "mathcal": {
            "prefix": "h",
            "command": "mathcal",
            "lower": false,
            "alternative": ["t"]
        },

        "mathscr": {
            "prefix": "k",
            "command": "mathscr",
            "lower": false
        },

        "sans-serif": {
            "prefix": "r",
            "command": "sf",
            "lower": true,
            "alternative": ["m", "q"]
        },

        "fraktur": {
            "prefix": "f",
            "command": "mathfrak",
            "lower": true,
            "alternative": ["i"]
        }
    },

    "sections": {

        "algebra": {
            "math-operators": {
                "gal": {
                    "comment": "The Galois group of a field extension"
                },
                "Frob": {
                    "comment": "Frob"
                }
            },
            "expression-aliases": {},
            "newcommands": {
                "ut": {
                    "command": "{#1}^\\times",
                    "comment": "Unit group of a ring"
                },
                "fix": {
                    "comment": "fix",
                    "command": "\\text{Fix}(#1)"
                }
            }
        },

        "algebraic-geometry": {
            "math-operators": {
                "spec": {
                    "comment": "The spectrum of a commutative ring"
                },
                "proj": {
                    "comment": "The Proj of a graded ring"
                }
            },
            "expression-aliases": {
                "ox": {
                    "comment": "The sheaf of regular functions on X",
                    "full-expr": "\\mathcal{O}_X"
                },
                "shv": {
                    "comment": "The category of sheaves over some space",
                    "full-expr": "{\\sf {Shv}}"
                }
            },
            "newcommands": {
                "I": {
                    "comment": "I",
                    "command": "\\mathcal{I}(#1)"
                },
                "V": {
                    "comment": "V",
                    "command": "\\mathcal{V}(#1)"
                },
                "Akn": {
                    "comment": "Akn",
                    "command": " {\\Bbb A}_k^#1"
                },
                "ARn": {
                    "comment": "ARn",
                    "command": " {\\Bbb A}_{\\Bbb R}^#1"
                },
                "An": {
                    "comment": "An",
                    "command": " {\\Bbb A}^#1"
                },
                "fr": {
                    "comment": "fr",
                    "command": "\\mathfrak #1"
                }
            }
        },

        "algebraic-number-theory": {
            "expression-aliases": {
                "galqp": {
                    "comment": "The absolute Galois group of \\Q_p",
                    "full-expr": "\\gal {\\overline{\\Q_p} } {\\Q_p} "
                },
                "galfp": {
                    "comment": "The absolute Galois group of \\F_p",
                    "full-expr": "\\gal {\\overline{\\F p} } {\\F p} "
                },
                "ok": {
                    "comment": "",
                    "full-expr": "\\mathcal O_K"
                },
                "ol": {
                    "comment": "",
                    "full-expr": "\\mathcal O_L"
                },
                "kpp": {
                    "comment": "",
                    "full-expr": "K_\\pp"
                },

                "idok": {
                    "comment": "",
                    "full-expr": "\\Id \\ok"
                },
                "clok": {
                    "comment": "",
                    "full-expr": "\\Cl \\ok"
                },
                "prinok": {
                    "comment": "",
                    "full-expr": "\\Prin \\ok"
                },
                "clk": {
                    "comment": "",
                    "full-expr": "\\Cl K"
                }
            },
            "newcommands": {
                "oo": {
                    "comment": "oo",
                    "command": "\\mathcal O_{#1} "
                },
                "Nm": {
                    "comment": "Nm",
                    "command": "\\text{Nm}(#1)"
                },
                "Tr": {
                    "comment": "",
                    "full-expr": "\\text{Tr} "
                },

                "Id": {
                    "comment": "Id",
                    "command": "\\text{Id}(#1)"
                },
                "Cl": {
                    "comment": "Cl",
                    "command": "\\text{Cl}(#1)"
                },
                "Prin": {
                    "comment": "Prin",
                    "command": "\\text{Prin}(#1)"
                },

                "ab": {
                    "comment": "ab",
                    "command": "#1^{\\text{ab} } "
                },
                "ur": {
                    "comment": "ur",
                    "command": "#1^{\\text{ur} } "
                }

            }
        },

        "algebraic-topology": {
            "math-operators": {
                "hocolim": {
                    "comment": "Homotopy colimit",
                    "custom-face": "sf"
                },
                "holim": {
                    "comment": "Homotopy limit",
                    "custom-face": "sf"
                }
            },
            "expression-aliases": {
                "wcc": {
                    "comment": "Weak equivalences in a model category",
                    "full-expr": "w\\CC"
                },
                "fcc": {
                    "comment": "Fibrations in a model category",
                    "full-expr": "f\\CC"
                },
                "cocc": {
                    "comment": "Cofibrations in a model category",
                    "full-expr": "co\\CC"
                },
                "Top": {
                    "comment": "",
                    "full-expr": " {\\sf{Top} } "
                },
                "hTop": {
                    "comment": "The homotopy category of $\\Top$",
                    "full-expr": " {\\sf{hTop} } "
                },
                "Ix": {
                    "comment": "Any index category",
                    "full-expr": " {\\sf{I} } "
                },
                "Topi": {
                    "comment": "A category of diagrams of shape I in Top",
                    "full-expr": " {\\Top^\\Ix} "
                }
            }
        },

        "category-theory": {
            "expression-aliases": {
                "ob": {
                    "comment": "",
                    "full-expr": " {\\sf{ob} } "
                },
                "isom": {
                    "comment": "",
                    "full-expr": "\\overset\\sim \\longrightarrow"
                }
            },
            "math-operators": {
                "colim": {
                    "comment": "Colimit",
                    "custom-face": "sf"
                },
                "cclim": {
                    "comment": "Limit",
                    "text": "lim",
                    "custom-face": "sf"
                },
                "Ext": {
                    "comment": "Ext functor"
                },
                "Tor": {
                    "comment": "Tor functor"
                }
            },
            "newcommands": {
                "hom": {
                    "command": "Hom_{#1}({#2},[#3])",
                    "comment": "Hom-sets in a category",
                    "arity": 3
                },
                "hhom": {
                    "comment": "hhom",
                    "command": " {\\sf{Hom} }(#1,-)"
                },
                "homm": {
                    "comment": "homm",
                    "command": " {\\sf{Hom} }(-,#1)"
                }
            }
        },


        "general": {
            "expression-aliases": {
                "Zgt": {
                    "comment": "Nonnegative integers",
                    "full-expr": "{\\mathbb{Z}}^{\\geq0}"
                },
                "Zi": {
                    "comment": "Gaussian integers",
                    "full-expr": "{\\mathbb{Z}}[i]"
                },
                "Ring": {
                    "comment": "",
                    "full-expr": " {\\sf{Ring} } "
                },
                "Set": {
                    "comment": "",
                    "full-expr": " {\\sf{Set} } "
                },
                "Grp": {
                    "comment": "",
                    "full-expr": " {\\sf{Grp} } "
                }
            },
            "math-operators": {
                "coker": {
                    "comment": "cokernel",
                    "upper": false
                },
                "im": {
                    "comment": "im"
                },
                "id": {
                    "comment": "id"
                },
                "Aut": {
                    "comment": "Aut"
                },
                "lcm": {
                    "comment": "lcm"
                }
            },
            "newcommands": {
                "hom": {
                    "command": "Hom_{#1}({#2},[#3])",
                    "comment": "Hom-sets in a category",
                    "arity": 3
                },
                "chn": {
                    "comment": "chn",
                    "command": "C_\\bullet(#1)"
                },
                "bkt": {
                    "comment": "bkt",
                    "command": "\\langle #1 \\rangle"
                },
                "bull": {
                    "comment": "bull",
                    "command": "A_\\bullet"
                }
            }
        }
    }
}
